{
    "name": "Path of Building Exporter",
    "version": "0.5",
    "description": "Export Garena PoE Characters to POB",
    "manifest_version": 3,
    "host_permissions": [
        "*://web.poe.garena.tw/character-window/*"
    ],
    "externally_connectable": {
        "matches": [
            "*://localhost/*",
            "*://localhost.localdomain/*",
            "*://void-battery.afq984.org/*"
        ]
    },
    "background": {
        "service_worker": "background.js"
    }
}
